<html>
  <head>
    <script src="https://code.jquery.com/jquery-2.2.1.min.js"></script>
    <script type="text/javascript">
    $(function() {
      $("table").each(function() {
        var $this = $(this);
        var newrows = [];
        $this.find("tr").each(function() {
          var i = 0;
          $(this).find("td").each(function(){
            i++;
            if(newrows[i] === undefined) {newrows[i] = $("<tr></tr>");}
            newrows[i].append($(this));
          });
        });
        $this.find("tr").remove();
        $.each(newrows, function(){
          $this.append(this);
        });
      });
    });
    </script>

  </head>
  <body>
    {{#grades}}
      <h1>{{name}}</h1>
      <table border="1">
        {{#domains}}
          <tr>
            {{#clusters}}
              <td>
                <ul>
                  {{#skills}}
                    <li>{{name}}</li>
                  {{/skills}}
                </ul>
              </td>
            {{/clusters}}
          </tr>
        {{/domains}}
      </table>
    {{/grades}}
  </body>
</html>
